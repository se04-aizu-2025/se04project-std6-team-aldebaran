<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.aldebaran.stellasort.controller.CountingSortController">
   <children>
      <BarChart fx:id="barChart" layoutX="20.0" layoutY="93.0" prefHeight="293.0" prefWidth="474.0">
        <xAxis>
          <CategoryAxis side="BOTTOM" />
        </xAxis>
        <yAxis>
          <NumberAxis side="LEFT" />
        </yAxis>
      </BarChart>
      <Button layoutX="77.0" layoutY="49.0" mnemonicParsing="false" onAction="#onPlaySortButtonClick" text="Sort" />
      <Button layoutX="135.0" layoutY="49.0" mnemonicParsing="false" onAction="#onSetInputButtonClick" text="SetInput" />
   </children>
</AnchorPane>
